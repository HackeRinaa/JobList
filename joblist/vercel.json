{
  "framework": "nextjs",
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "routes": [
    {
      "src": "/worker",
      "dest": "/worker",
      "continue": true,
      "headers": {
        "x-middleware-skip": "true"
      }
    }
  ],
  "env": {
    "NEXT_SKIP_CHECKS": "1"
  }
} 